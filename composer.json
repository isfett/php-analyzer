{
    "name": "isfett/php-analyzer",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Stenke",
            "email": "chris@isfett.com"
        }
    ],
    "support": {
        "issues": "https://github.com/isfett/php-analyzer/issues"
    },
    "bin": ["bin/php-analyzer"],
    "require": {
        "php": "^7.1",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/finder": "^3.4 || ^4.0",
        "nikic/php-parser": "^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.3",
        "symfony/config": "^3.4 || ^4.3",
        "symfony/http-kernel": "^3.4 || ^4.3",
        "symfony/yaml": "^3.4 || ^4.3",
        "doctrine/collections": "^1.6",
        "symfony/event-dispatcher": "^3.4 || ^4.3",
        "symfony/serializer": "^3.4 || 4.4.x-dev"
    },
    "require-dev": {
        "isfett/coding-standard": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpdoc-parser": "0.3.5 - 0.4.2"
    },
    "autoload": {
        "psr-4": {
            "Isfett\\PhpAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isfett\\PhpAnalyzer\\Tests\\": "tests/",
            "SlevomatCodingStandard\\": "vendor/isfett/coding-standard/phpcs/SlevomatCodingStandard"
        }
    }
}
